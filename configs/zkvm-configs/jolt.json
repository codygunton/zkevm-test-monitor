{
  "name": "jolt",
  "repository": "https://github.com/codygunton/jolt",
  "commit": "c4b9b060",
  "build": {
    "docker": true,
    "command": "cargo build --release -p tracer --bin jolt-emu",
    "binary_path": "target/release/jolt-emu",
    "environment": {
      "RUST_LOG": "info"
    }
  },
  "plugin": {
    "name": "jolt",
    "path": "plugins/jolt"
  },
  "capabilities": {
    "note": "ISA information is determined from the plugin's ISA YAML file",
    "plugin_isa_file": "plugins/jolt/jolt_isa.yaml"
  }
}