{
  "name": "SP1",
  "repository": "https://github.com/codygunton/sp1",
  "commit": "fc98075a",
  "build": {
    "command": "cargo build --bin sp1-perf-executor",
    "binary_path": "target/debug/sp1-perf-executor",
    "environment": {
      "RUST_LOG": "info"
    }
  },
  "plugin": {
    "name": "sp1",
    "path": "plugins/sp1"
  },
  "capabilities": {
    "note": "ISA information is determined from the plugin's ISA YAML file",
    "plugin_isa_file": "plugins/sp1/sp1_isa.yaml"
  }
}